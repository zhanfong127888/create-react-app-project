@import './css/markdown.wxss';
@import './css/popup.wxss';
/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.pop-readme {
  display: flex !important;
  flex-direction: column;
  align-items: center;
  padding-top: 30px !important;
  box-sizing: border-box;
  overflow: auto;
}

.pop-readme .ul{
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.pop-readme .ul image{
  width: 300px;
  height: 300px;
}