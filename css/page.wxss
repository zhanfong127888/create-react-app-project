}
.actionMask.show {
  animation: fadeIn ease 0.3s forwards;
  z-index: 1;
  display: block;
}
.as-common,
.actionSide-pop,
.actionSide-pop-top,
.actionSide-pop-bottom,
.actionSide-toast,
.actionSide-right,
.actionSide-left,
.actionSide-bot,
.actionSide-top,
.actionSide-message {
