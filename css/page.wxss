.btn,
.item-btn,
.item-dropdown {
  border: 1px solid #1b9af7;
  background-color: #1b9af7;
  border-radius: 5px;
  height: 60px;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 15px;
  transition: all 0.2s;
  position: relative;
  color: #fff;
}
.btn:hover,
.item-btn:hover,
.item-dropdown:hover {
  background-color: #4cb0f9;
  border-color: #2798eb;
  color: #fff;
}
.btn:active,
.item-btn:active,
.item-dropdown:active {
  transform: scale(0.98, 0.98);
  background-color: #2798eb;
  border-color: #2798eb;
  color: #0880d7;
  text-shadow: 0 1px 0 rgba(255,255,255,0.3);
}
.list-funs {
  margin-top: 30px;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.list-funs .fun {
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 8px;
  border: 1px solid #999;
  border-radius: 10px;
  margin: 5px;
}
.list-funs .fun.primary {
  background-color: #337ab7;
  color: #fff;
}
.list-funs .fun.success {
  background-color: #dff0d8;
}
.list-funs .fun.info {
  background-color: #afd9ee;
}
.list-funs .fun.warning {
  background-color: #fcf8e3;
}
.list-funs .fun.danger {
  background-color: #f2dede;
}
.list-funs .fun:active {
  border: none;
  transform: scale(0.99, 0.99);
  box-shadow: 0 0 0 1px #ddd inset;
}
.seeCode {
  border: 1px solid #ccc;
  background-color: #ccc;
  border-radius: 5px;
  height: 60px;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 15px;
  transition: all 0.2s;
  position: relative;
  color: #000;
  width: 100px;
  height: 50px;
}
@keyframes loader-01 {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
@keyframes slideUp {
  from {
    transform: translate3d(0, 100%, 0);
  }
  to {
    transform: translate3d(0, 0, 0);
  }
}
@keyframes slideDown {
  from {
    transform: translate3d(0, -100%, 0);
  }
  to {
    transform: translate3d(0, 0, 0);
  }
}
@keyframes slideRight {
  from {
    transform: translate3d(100%, 0, 0);
  }
  to {
    transform: translate3d(0, 0, 0);
  }
}
@keyframes slideLeft {
  from {
    transform: translate3d(-100%, 0, 0);
  }
  to {
    transform: translate3d(0, 0, 0);
  }
}
@keyframes scalePop {
  from {
    transform: scale(0.97);
  }
  to {
    transform: scale(1);
  }
}
.boxer {
  position: relative;
}
.sticky-button {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 60px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #000;
  color: #fff;
  font-size: 20px;
}
.item-btn {
  margin: 0 auto;
  margin-top: 60px;
  width: 200px;
}
.item-btn:after {
  border: 3px dotted currentcolor;
  border-radius: 50%;
  content: '';
  display: none;
  width: 20px;
  height: 20px;
  position: absolute;
  top: 50%;
  left: 15%;
  margin-top: -13px;
}
.item-btn.loading:after {
  display: block;
  animation: 1s loader-01 linear infinite;
}
.item-dropdown {
  margin: 0 auto;
  margin-top: 60px;
  width: 160px;
  height: 40px;
  position: relative;
}
.item-dropdown:before {
  content: '▶︎';
  position: absolute;
  top: 12px;
  right: 8px;
  font-size: 12px;
  color: #efefef;
}
.item-dropdown .hbody {
  border: 1px solid #eee;
  border-radius: 5px;
  position: absolute;
  display: none;
  top: 44px;
  left: 0px;
  background-color: #fff;
  color: #000;
  width: 100%;
}
.item-dropdown .hbody .hb-item {
  padding: 10px;
}
.item-dropdown .hbody .hb-item.selected {
  background-color: #eee;
  color: #f00;
}
.item-dropdown.active .hbody {
  display: block;
}
.item-dropdown.active:before {
  content: '▼';
}
